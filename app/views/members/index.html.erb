<%= form_tag :search_members, method: :get do %>
  <p>
    <%= text_field_tag 'q', params[:q] %>
    <%= submit_tag 'search' %>
  </p>
<% end %>
  

<% if @members.present? %>
  <table class="list">
    <tr>
      <th>number</th>
      <th>nickname</th>
      <th>name</th>
      <th>email</th>
      <th>operation</th>
    </tr>

    <% @members.each do |member| %>
      <tr>
        <td><%= member.number %></td>
        <td><%= link_to member.name, member %></td>
        <td><%= member.full_name %></td>
        <td><%= member.email %></td>
      </tr>
    <% end %>
  </table>
<% end %>

<%= will_paginate @members %>
