<%= link_to 'new member', :new_member %>

<% if @members.present? %>
  <table class="list">
    <tr>
      <th>number</th>
      <th>nickname</th>
      <th>name</th>
      <th>operation</th>
    </tr>

    <% @members.each do |member| %>
      <tr>
        <td><%= member.number %></td>
        <td><%= link_to member.name, member %></td>
        <td><%= member.full_name %></td>
        <td>
          <%= link_to 'edit', [:edit, member] %>
          <%= link_to 'delete', member, method: :delete, confirm: 'really delete?' %>
      </td>
      </tr>
    <% end %>
  </table>
<% end %>


